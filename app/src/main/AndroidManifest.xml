<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.zorax.wms">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wms"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/wms"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".check.day.DayCheckActivity"></activity>
        <activity android:name=".check.CheckIndexActivity" />
        <activity android:name=".setting.SettingActivity" />
        <activity android:name=".index.IndexActivity" />
        <activity android:name=".instore.instoreView.InstoreActivity" />
        <activity android:name=".instore.index.InstoreIndexActivity" />
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>